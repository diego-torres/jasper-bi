<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>MetasVenta</display-name>
  <servlet>
  	<servlet-name>SellerController</servlet-name>
  	<display-name>SellerController</display-name>
  	<description></description>
    <servlet-class>com.inspiracode.praxma.rhi.web.saleGoals.controller.SellerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StoreController</servlet-name>
  	<display-name>StoreController</display-name>
  	<description></description>
  	<servlet-class>com.inspiracode.praxma.rhi.web.saleGoals.controller.StoreController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SellerController</servlet-name>
  	<url-pattern>/SellerController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StoreController</servlet-name>
  	<url-pattern>/StoreController</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
