	<queryString language="SQL">
		<![CDATA[SELECT dim_grupo_vencimiento.minimo_dias || '-' || dim_grupo_vencimiento.maximo_dias AS Grupo
	,COALESCE(Suma_Saldo.vencido, 0) AS vencido
	,COALESCE(Suma_Saldo.vencido, 0) / (
		SELECT SUM(fact_vencido.saldo_vencido) AS vencido
		FROM dim_grupo_vencimiento
		LEFT JOIN fact_vencido ON fact_vencido.id_grupo_vencimiento = dim_grupo_vencimiento.id_grupo_vencimiento
		LEFT JOIN dim_clientes ON dim_clientes.id_cliente = fact_vencido.id_cliente
		WHERE dim_clientes.es_local = true
		) AS Porcentaje
FROM dim_grupo_vencimiento
LEFT JOIN (
	SELECT SUM(fact_vencido.saldo_vencido) AS vencido
		,dim_grupo_vencimiento.minimo_dias || '-' || dim_grupo_vencimiento.maximo_dias AS Grupo
	FROM dim_grupo_vencimiento
	LEFT JOIN fact_vencido ON fact_vencido.id_grupo_vencimiento = dim_grupo_vencimiento.id_grupo_vencimiento
	LEFT JOIN dim_clientes ON dim_clientes.id_cliente = fact_vencido.id_cliente
	WHERE dim_clientes.es_local = false
	GROUP BY Grupo
	) AS Suma_Saldo ON Suma_Saldo.Grupo = dim_grupo_vencimiento.minimo_dias || '-' || dim_grupo_vencimiento.maximo_dias
ORDER BY Grupo ASC]]>
	</queryString>
