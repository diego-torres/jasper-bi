	<queryString language="SQL">
		<![CDATA[SELECT dim_clientes.es_local,
	fact_vencido.saldo_vencido,
	fact_vencido.dias_vencimiento,
	minimo_dias || ' - ' || maximo_dias AS Grupo
FROM dim_grupo_vencimiento
	 INNER JOIN fact_vencido ON 
	 fact_vencido.id_grupo_vencimiento = dim_grupo_vencimiento.id_grupo_vencimiento 
	 INNER JOIN dim_clientes ON 
	 dim_clientes.id_cliente = fact_vencido.id_cliente
WHERE dim_clientes.es_local = false]]>
